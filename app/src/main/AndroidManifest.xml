<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pomodoro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pomodoro">
        <activity android:name=".EditarPomodorosActivity"></activity>
        <activity android:name=".ComenzarPomodoroActivity" />
        <activity
            android:name=".PerfilActivity"
            android:label="@string/title_activity_perfil"
            android:theme="@style/Theme.Pomodoro.NoActionBar" />
        <activity android:name=".CalendarioActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".NuevoPomodoroActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>