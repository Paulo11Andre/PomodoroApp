<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pomodoro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_pomodoro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_pomodoro_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pomodoro">
        <activity android:name=".ConfiguracionActivity" />
        <activity android:name=".EditarPomodorosActivity" />
        <activity android:name=".ComenzarPomodoroActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".CalendarioActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".NuevoPomodoroActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".InicioActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>